<h1>Logic#index</h1>
<p>Find me in app/views/logics/index.html.erb</p>

<div class="margin-top: 0.75rem;" id="result-div">    
    <table border="1" cellspacing="0" cellpadding="0" width="100px" >
    <tr>
        <td> id </td>
        <td> A </td>
        <td> B </td>
        <td> nod </td>
        <td> nok </td>
        <td></td>
    </tr>
    <% @logics.each do |l| %>
        <tr>
            <td> <%= l.id %></td>
            <td> <%= l.num_a %></td>
            <td> <%= l.num_b %></td>
            <td> <%= l.result['nod'] %></td>
            <td> <%= l.result['nok'] %></td>
            <td><%= button_to 'Удалить', l, method: :delete %></td>
        </tr>
    <% end %>
    </table>
    <%= link_to 'Добавить', new_logic_path %>
    <%= link_to 'XML', logics_result_path, :method => "get" %>
</div>
<%# console %>